{
    "presets":["es2015","react"],
    "plugins": [["transform-class-properties", { "spec": true }]],
    "env": {
        "development": {
            "plugins": [
                ["react-transform", {
                    "transforms": [{
                        "transform": "react-transform-hmr",
                        "imports": ["react"],
                        "locals": ["module"]
                    }]
                }]
            ]
         }
     }
 }
